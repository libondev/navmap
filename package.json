{
  "name": "navmap",
  "version": "0.0.1",
  "description": "Show all the titles on your page in the scrollbar.",
  "type": "module",
  "keywords": [
    "minimap",
    "canvas",
    "navigator",
    "toc",
    "table-of-content"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "fix": "eslint src/** --ext .js,.ts,.cjs --fix"
  },
  "author": "libondev <bon.li@outlook.com>",
  "bugs": "https://github.com/libondev/navmap/issues",
  "homepage": "https://github.com/libondev/navmap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libondev/navmap.git"
  },
  "volta": {
    "node": "16.18.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.43.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  }
}
